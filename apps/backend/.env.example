# Server Configuration
PORT=3001
NODE_ENV=development

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/web3-gaming
REDIS_URL=redis://localhost:6379
DYNAMODB_REGION=us-east-1
DYNAMODB_TABLE_PREFIX=web3-gaming

# Blockchain Configuration
IMMUTABLE_X_API_KEY=your_immutable_x_api_key
RONIN_RPC_URL=https://api.roninchain.com/rpc
FLOW_ACCESS_NODE=https://rest-mainnet.onflow.org
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Chainlink Oracle
CHAINLINK_PRICE_FEED_ADDRESS=0x0000000000000000000000000000000000000000
CHAINLINK_VRF_COORDINATOR=0x0000000000000000000000000000000000000000

# JWT
JWT_SECRET=your_jwt_secret_key_change_in_production
JWT_EXPIRATION=7d

# CORS
CORS_ORIGIN=http://localhost:3000

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100

# Colyseus
COLYSEUS_PORT=2567
COLYSEUS_MONITOR_AUTH=admin:password

# WebRTC
WEBRTC_TURN_SERVER=turn:turnserver.example.com:3478
WEBRTC_TURN_USERNAME=username
WEBRTC_TURN_CREDENTIAL=password
